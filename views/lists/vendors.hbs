<section>
    <h1>Поставщики:</h1>
</section>
<table class="table">
    <thead>
    <tr>
        <th>id</th>
        <th>Фамилия</th>
        <th>Имя</th>
        <th>Отчество</th>
        <th>Компания</th>
        <th>Email</th>
        <th>phone</th>
        <th>Действия</th>
    </tr>
    </thead>
    <tbody>
    {{#each vendors}}
        <tr>
            <td>{{this.id}}</td>
            <td>{{this.last_name}}</td>
            <td>{{this.name}}</td>
            <td>{{this.patronymic}}</td>
            <td>{{this.company}}</td>
            <td>{{this.email}}</td>
            <td>{{this.phone}}</td>
            <td>
                <div class="actions">
                    <a class="edit" href="edit/{{this.id}}">
                        <img src="../images/edit.svg" alt="edit">
                    </a>
                    <input type="image" src="../images/delete.svg" name="delete" alt="delete"/>
                </div>
            </td>
        </tr>
    {{/each}}
    </tbody>
</table>
